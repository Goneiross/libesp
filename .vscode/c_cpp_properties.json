{
    "configurations": [
        {
            "name": "ESP32-Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "~/esp/**"
            ],
            "intelliSenseMode": "clang-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "~/esp/**"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/**.vscode/browse.vc.db"
            }
        }
    ],
    "version": 4
}